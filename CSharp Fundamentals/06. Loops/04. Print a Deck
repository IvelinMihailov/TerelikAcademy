using System;

class Print_Deck
{
    static void Main()
    {

        string strN = Console.ReadLine().ToUpper();
        int counter = 0;
        int n = 0;

        switch (strN)
        {
            case "1": n = 1; break;
            case "2": n = 2; break;
            case "3": n = 3; break;
            case "4": n = 4; break;
            case "5": n = 5; break;
            case "6": n = 6; break;
            case "7": n = 7; break;
            case "8": n = 8; break;
            case "9": n = 9; break;
            case "10": n = 10; break;
            case "J": n = 11; break;
            case "Q": n = 12; break;
            case "K": n = 13; break;
            case "A": n = 14; break;

        }

        for (int cards = 1; cards < n; cards++)
        {
            switch (cards)
            {
                case 1:
                    counter = 2;
                    Console.WriteLine("{0} of spades, {0} of clubs, {0} of hearts, {0} of diamonds", counter);
                    break;
                case 2:
                    counter = 3;
                    Console.WriteLine("{0} of spades, {0} of clubs, {0} of hearts, {0} of diamonds", counter);
                    break;
                case 3:
                    counter = 4;
                    Console.WriteLine("{0} of spades, {0} of clubs, {0} of hearts, {0} of diamonds", counter);
                    break;
                case 4:
                    counter = 5;
                    Console.WriteLine("{0} of spades, {0} of clubs, {0} of hearts, {0} of diamonds", counter);
                    break;
                case 5:
                    counter = 6;
                    Console.WriteLine("{0} of spades, {0} of clubs, {0} of hearts, {0} of diamonds", counter);
                    break;
                case 6:
                    counter = 7;
                    Console.WriteLine("{0} of spades, {0} of clubs, {0} of hearts, {0} of diamonds", counter);
                    break;
                case 7:
                    counter = 8;
                    Console.WriteLine("{0} of spades, {0} of clubs, {0} of hearts, {0} of diamonds", counter);
                    break;
                case 8:
                    counter = 9;
                    Console.WriteLine("{0} of spades, {0} of clubs, {0} of hearts, {0} of diamonds", counter);
                    break;
                case 9:
                    counter = 10;
                    Console.WriteLine("{0} of spades, {0} of clubs, {0} of hearts, {0} of diamonds", counter);
                    break;
                case 10:
                    counter = 11;
                    Console.WriteLine("J of spades, J of clubs, J of hearts, J of diamonds");
                    break;
                case 11:
                    counter = 12;
                    Console.WriteLine("Q of spades, Q of clubs, Q of hearts, Q of diamonds");

                    break;
                case 12:
                    counter = 13;
                    Console.WriteLine("K of spades, K of clubs, K of hearts, K of diamonds");

                    break;
                case 13:
                    counter = 14;
                    Console.WriteLine("A of spades, A of clubs, A of hearts, A of diamonds");

                    break;
                default:
                    break;

            }

        }

    }
}
